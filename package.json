{
   "name": "sockii",
   "version": "0.9.14",
   "description": "HTTP and WebSocket aggregation thinlayer",
   "author": {
       "name": "Server Density",
       "email": "hello@serverdensity.com",
       "web": "http://www.serverdensity.com/"
   }, 
   "repository": {
       "type": "git",
       "url": "git@github.com:serverdensity/sockii.git" 
   }, 
   "engines": {
       "node": ">= 0.8"
   },
   "bin": {
       "sockii": "./bin/sockii"
   },
   "dependencies": {
       "socket.io": "0.9.13",
       "ws": "0.4.25",
       "coffee-script": "1.3.*",
       "lodash": "0.4.*",
       "http-proxy": "0.10.*",
       "request": "2.9.*",
       "mongodb": "1.2.*",
       "socket.io-client": "0.9.*",
       "options": "*",
       "winston": "0.6.*",
       "longjohn": "0.0.*",
       "async": "0.1.22",
       "optimist": "0.3.*",
       "node-uuid": "1.4.*"
   },
   "devDependencies": {
       "vows": "0.5.*",
       "nock": "0.13.*"
   },
   "scripts": {
       "test": "vows tests/*-test.coffee --spec",
       "xunit-tests": "echo > tests.log && TESTOUTPUT=`vows tests/*-test.coffee --xunit` && cat ./tests.log && echo && mkdir -p ./test-reports && echo $TESTOUTPUT > ./test-reports/socky.xml"
   },
   "directories": {
       "lib": "src"
   },
   "main": "./src/run-sockii"
}
